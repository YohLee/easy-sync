<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE baseframe:jproxy PUBLIC
    "-//baseframe:jproxy//DTD baseframe:jproxy Configuration 1.0//EN"
    "http://localhost/baseframe-jproxy-1.0.dtd">
<baseframe:jproxy>
	
	<properties>
		<!-- 本地服务端口 -->
		<property name="local_service_port" value="9999"/>
		<!-- 服务类获取工具，该类由服务端实现 BeanProvider接口-->
		<property name="bean_provider" value="com.hty.util.filesync.util.SimpleServiceProvider"/>
	</properties>
	
	<service:local>
		<lservice class="com.hty.util.filesync.service.FileService"></lservice>
	</service:local>

</baseframe:jproxy>